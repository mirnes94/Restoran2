            android:label="@string/app_name" />
        <activity
            android:name="com.lakj.comspace.simpletextclient.FoodLab"
            android:label="Food Lab" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.lakj.comspace.simpletextclient.Order_Type" />
        <activity
            android:name="com.lakj.comspace.simpletextclient.finalize_order"
            android:label="@string/title_activity_finalize_order" />
